{
    "name": "clickandmortar/feeder",
    "description": "PHP data feeder for Elasticsearch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael BOUVY",
            "email": "michael.bouvy@clickandmortar.fr"
        }
    ],
    "require": {
        "elasticsearch/elasticsearch": "6.*",
        "symfony/console": "~3.4",
        "symfony/config": "~3.4",
        "symfony/yaml": "~3.4",
        "symfony/dependency-injection": "~3.4",
        "symfony/dotenv": "^4.0",
        "symfony/finder": "^4.1"
    },
    "autoload": {
        "psr-4": { "Feeder\\": "src/" }
    },
    "bin": ["bin/feeder"],
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "willdurand/geocoder": "Allows usage of Geo service (^3.2)",
        "geoip2/geoip2": "Allows using IP geocoding in Geo service (^2.4)"
    }
}
